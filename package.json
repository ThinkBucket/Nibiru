{
  "name": "thinkbucket",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "lint": "eslint --cache \"**/*.js\"",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,ts}\"",
    "prettier:diff": "prettier --config .prettierrc --list-different \"**/*.{js,ts}\"",
    "prettier-docs": "prettier --config .prettierrc --write \"**/*.md\"",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn lint --fix",
      "yarn prettier"
    ],
    "*.{ts,tsx}": [
      "yarn prettier"
    ],
    "*.md": [
      "yarn prettier-docs"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.18",
    "@docusaurus/plugin-pwa": "^2.0.0-beta.18",
    "@docusaurus/preset-classic": "2.0.0-beta.18",
    "@docusaurus/theme-live-codeblock": "2.0.0-beta.18",
    "@easyops-cn/docusaurus-search-local": "^0.23.0",
    "@http-util/request": "^0.1.0",
    "classnames": "^2.3.1",
    "npm-to-yarn": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lifecycles-compat": "^3.0.4",
    "react-toggle": "^4.1.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-beta.18",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/react": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.3.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.3",
    "prettier": "^2.1.2",
    "typescript": "^4.1.4"
  }
}
